ALTER TABLE CE_CUPOMBRINDE
ADD 
VL_DESCONTOPROMO NUMERIC(5,2),
FK_PRODUTOPROMO CHAR(15),
QT_MINPROMO INTEGER,
QT_MAXPRMO INTEGER

;

UPDATE CE_CUPOMBRINDE SET
VL_DESCONTOPROMO=0
WHERE VL_DESCONTOPROMO IS NULL
;

UPDATE CE_CUPOMBRINDE SET
FK_PRODUTOPROMO=' '
WHERE FK_PRODUTOPROMO IS NULL
;

UPDATE CE_CUPOMBRINDE SET
QT_MINPROMO=0
WHERE QT_MINPROMO IS NULL
;

UPDATE CE_CUPOMBRINDE SET
QT_MAXPRMO=0
WHERE QT_MAXPRMO IS NULL